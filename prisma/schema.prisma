
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model File {
  id        String   @id @default(uuid())
  fileId    String   @unique
  fileName  String
  nomorSurat String
  jenisSurat String
  tanggalDibuat DateTime
  createdAt DateTime @default(now())
}
